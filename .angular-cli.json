{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "findeo-angular"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "assets/css/style.css",
        "assets/css/colors-main.css",
        "assets/css/rtl-config.css",
        "assets/css/jquery.modal.min.css",
        "../node_modules/toastr/build/toastr.min.css"
      ],
      "scripts": [
        "assets/scripts/jquery-2.2.0.min.js",
        "assets/scripts/bootstrap.min.js",
        "assets/scripts/dropzone.js",
        "assets/scripts/chosen.min.js",
        "assets/scripts/magnific-popup.min.js",
        "assets/scripts/owl.carousel.min.js",
        "assets/scripts/rangeSlider.js",
        "assets/scripts/sticky-kit.min.js",
        "assets/scripts/slick.min.js",
        "assets/scripts/mmenu.min.js",
        "assets/scripts/tooltips.min.js",
        "assets/scripts/masonry.min.js",
        "assets/scripts/custom.js",
        "assets/scripts/rtl-config.js",
        "assets/scripts/infobox.min.js",
        "assets/scripts/markerclusterer.js",
        "assets/scripts/jquery.modal.min.js",
        "../node_modules/toastr/build/toastr.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "class": {
      "spec": false
    },
    "component": {
      "spec": false
    },
    "directive": {
      "spec": false
    },
    "guard": {
      "spec": false
    },
    "module": {
      "spec": false
    },
    "pipe": {
      "spec": false
    },
    "service": {
      "spec": false
    }
  }
}
